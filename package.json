{
  "name": "forkspy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:dev:build": "docker build -f Dockerfile.dev -t forkspy-app:latest .",
    "docker:dev:detached": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:stop": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker logs forkspy-app",
    "docker:shell": "docker exec -it forkspy-app sh",
    "docker:test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit",
    "docker:test:watch": "docker-compose -f docker-compose.test.yml --profile watch up",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down",
    "docker:prod": "docker-compose up -d",
    "docker:prod:build": "docker-compose build --no-cache",
    "docker:prod:logs": "docker-compose logs -f",
    "docker:prod:down": "docker-compose down",
    "docker:prod:proxy": "docker-compose --profile proxy up -d",
    "ci:docker:build": "docker build -t forkspy-ci:latest .",
    "ci:docker:test": "docker run --rm -e NODE_ENV=test forkspy-ci:latest npm run test:coverage",
    "ci:docker:security": "docker run --rm -v /var/run/docker.sock:/var/run/docker.sock aquasec/trivy image forkspy-ci:latest",
    "ci:docker:push": "docker push $npm_config_registry/forkspy:latest"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "framer-motion": "^12.4.2",
    "lucide-react": "^0.475.0",
    "mongodb": "^6.12.0",
    "next": "15.1.4",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resend": "^4.1.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "jsdom": "^26.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tailwindcss-scrollbar": "^0.1.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
